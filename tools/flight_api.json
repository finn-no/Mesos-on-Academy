{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "tutum.co/stigkj/flight_api",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 8080, "hostPort": 0, "servicePort": 81, "protocol": "tcp" }
      ],
      "parameters": [
        { "key": "env", "value": "CONSTRETTO_TAGS=local" }
      ]
    }
  },
  "id": "flightapi",
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "uris": []
}

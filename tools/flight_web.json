{
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "tutum.co/stigkj/flight_web",
      "network": "BRIDGE",
      "portMappings": [
        { "containerPort": 80, "hostPort": 0, "servicePort": 80, "protocol": "tcp" }
      ],
      "parameters": [
        { "key": "env", "value": "CONSTRETTO_TAGS=local" }
      ]
    }
  },
  "id": "flightweb",
  "instances": 1,
  "cpus": 0.25,
  "mem": 256,
  "uris": []
}
